{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ordermanagement/src",
  "monorepo": true,
  "root": "apps/ordermanagement",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/ordermanagement/tsconfig.app.json"
  },
  "projects": {
    "ordermanagement": {
      "type": "application",
      "root": "apps/ordermanagement",
      "entryFile": "main",
      "sourceRoot": "apps/ordermanagement/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ordermanagement/tsconfig.app.json"
      }
    },
    "orders": {
      "type": "application",
      "root": "apps/orders",
      "entryFile": "main",
      "sourceRoot": "apps/orders/src",
      "compilerOptions": {
        "tsConfigPath": "apps/orders/tsconfig.app.json"
      }
    },
    "payment": {
      "type": "application",
      "root": "apps/payment",
      "entryFile": "main",
      "sourceRoot": "apps/payment/src",
      "compilerOptions": {
        "tsConfigPath": "apps/payment/tsconfig.app.json"
      }
    },
    "products": {
      "type": "application",
      "root": "apps/products",
      "entryFile": "main",
      "sourceRoot": "apps/products/src",
      "compilerOptions": {
        "tsConfigPath": "apps/products/tsconfig.app.json"
      }
    }
  }
}